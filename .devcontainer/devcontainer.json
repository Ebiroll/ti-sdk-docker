{
  "name": "TI SDK Dev Container",
  "build": {
    "dockerfile": "../ti-processor-sdk-linux-edgeai-j722s-evm-11_00_00_08/Dockerfile",
    "context": "../ti-processor-sdk-linux-edgeai-j722s-evm-11_00_00_08"
  },
  "image": "ghcr.io/ebiroll/ti-sdk-docker:latest",
  "workspaceFolder": "/home/tisdk/ti",
  "remoteUser": "tisdk",
  "runArgs": [
    "--env", "DISPLAY=${env:DISPLAY}",
    "--volume", "/tmp/.X11-unix:/tmp/.X11-unix",
    "--volume", "${localWorkspaceFolder}:/home/tisdk/ti"
  ],
  "updateRemoteUserUID": false,
  "mounts": [],
  "postCreateCommand": "echo 'Dev container ready.'"
}
